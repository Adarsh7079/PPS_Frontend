        "common": [
            "ej2", 
            "ej2-pdfviewer", 
            "syncfusion", 
            "pdf-viewer", 
             "typescript"
        ],
        "angular": [
            "angular", 
			"angular-pdf-viewer"
        ],
        "react": [
            "react", 
            "reactjs", 
            "react-pdf-viewer",
			"javascript"
        ],
        "vue": [ 
            "vue", 
            "vuejs", 
            "vue-pdf-viewer" 
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/syncfusion/ej2-pdfviewer"
